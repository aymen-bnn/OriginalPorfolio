{
  "compilerOptions": {
    "target": "es5",
    "lib": ["dom", "dom.iterable", "esnext"],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "incremental": true
  },
  "include": ["next-env.d.ts", "**/*.ts", "**/*.tsx", "pages/Components/About/About.jsx", "pages/Components/Expertise/Expertise.jsx", "pages/Components/NavBar/DrawerCols.jsx", "pages/Components/Landing/Particles.jsx", "pages/Components/Landing/Landing.jsx", "pages/Components/NavBar/NavBar.jsx", "pages/Components/Contact/Contact.jsx"],
  "exclude": ["node_modules"]
}
